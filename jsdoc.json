{
    "plugins": [
        "plugins/markdown",
        "plugins/summarize"
    ],
    "recurseDepth": 10,
    "source": {
        "include": [
            "README.md",
            "main.js",
            "dataSource",
            "middlewares",
            "controllers",
            "localScripts",
            "cronJobs",
            "utilities"
        ],
        "exclude": [
            "node_modules",
            "public",
            ".vscode",
            "assets",
            "controllers/restApi/apiDoc/jsdoc"
        ],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "opts": {
        "template": "node_modules/docdash",
        "encoding": "utf8",
        "destination": "./controllers/restApi/apiDoc/jsdoc/",
        "recurse": true
    }
}